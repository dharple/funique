{
    "name": "outsanity/funique",
    "description": "Finds files unique to one directory or another",
    "type": "project",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Doug Harple",
            "email": "dharple@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.29",
        "symfony/console": "^6.4.27",
        "symfony/flex": "^2.10.0"
    },
    "require-dev": {
        "outsanity/phpcs": ">=0.4.0",
        "phpstan/phpstan": "^1.12.32",
        "phpunit/phpunit": "^10.5.58",
        "ramsey/uuid": "^4.9.1",
        "rector/rector": "^0.16.0",
        "symfony/dotenv": "^6.4.24",
        "symfony/filesystem": "^6.4.24"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.29"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true
        },
        "audit": {
            "abandoned": "report"
        }
    },
    "autoload": {
        "psr-4": {
            "Outsanity\\Funique\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Outsanity\\Tests\\Funique\\": "tests/"
        }
    },
    "scripts": {
        "compile":  "bin/compile",
        "phpcbf":   "phpcbf",
        "phpcs":    "phpcs",
        "phpstan":  "phpstan analyse",
        "rector":   "rector",
        "test":     "phpunit"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
