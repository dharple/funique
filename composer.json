{
    "name": "dharple/funique",
    "description": "Finds files unique to one directory or another",
    "type": "package",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Doug Harple",
            "email": "dharple@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^5.1"
    },
    "require-dev": {
        "outsanity/phpcs": "*",
        "phpstan/phpstan": "^0.12.48"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Funique\\": "src/"
        }
    },
    "scripts": {
        "phpcbf":  "phpcbf",
        "phpcs":   "phpcs",
        "phpstan": "phpstan analyse"
    }
}
