{
    "name": "outsanity/funique",
    "description": "Finds files unique to one directory or another",
    "type": "package",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Doug Harple",
            "email": "dharple@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.2",
        "symfony/console": "^6.4.11",
        "symfony/flex": "^2.4.6"
    },
    "require-dev": {
        "outsanity/phpcs": ">=0.4.0",
        "phpstan/phpstan": "^1.12.0",
        "phpunit/phpunit": "^10.5.30",
        "ramsey/uuid": "^4.7.6",
        "rector/rector": "^0.16.0",
        "symfony/filesystem": "^6.4.9"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true
        },
        "audit": {
            "abandoned": "fail"
        }
    },
    "autoload": {
        "psr-4": {
            "Outsanity\\Funique\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Outsanity\\Tests\\Funique\\": "tests/"
        }
    },
    "scripts": {
        "compile":  "bin/compile",
        "phpcbf":   "phpcbf",
        "phpcs":    "phpcs",
        "phpstan":  "phpstan analyse",
        "rector":   "rector",
        "test":     "phpunit"
    }
}
